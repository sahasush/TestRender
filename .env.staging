# Staging Environment Configuration
# This file is used for staging/testing deployment

# API Configuration
VITE_API_URL=https://staging-api.eirvana.com
VITE_API_TIMEOUT=20000
VITE_API_KEY=staging-api-key-12345
VITE_API_VERSION=v1

# Application Settings
VITE_APP_NAME=Eirvana (Staging)
VITE_APP_VERSION=1.0.0
VITE_APP_ENVIRONMENT=staging

# Feature Flags
VITE_ENABLE_DEBUG=true
VITE_ENABLE_MOCK_AUTH=false
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_ERROR_REPORTING=true

# Authentication Settings
VITE_AUTH_TOKEN_EXPIRY=86400000
VITE_AUTH_REFRESH_THRESHOLD=300000

# UI Configuration
VITE_DEFAULT_THEME=light
VITE_ENABLE_THEME_TOGGLE=true
VITE_SHOW_VERSION_INFO=true

# Development Tools (Enabled for Testing)
VITE_ENABLE_REACT_DEVTOOLS=true
VITE_ENABLE_REDUX_DEVTOOLS=true
VITE_LOG_LEVEL=info

# External Services (Staging)
VITE_SENTRY_DSN=https://your-staging-sentry-dsn@sentry.io/project
VITE_GOOGLE_ANALYTICS_ID=
VITE_MIXPANEL_TOKEN=

# Health Check
VITE_HEALTH_CHECK_INTERVAL=45000
VITE_HEALTH_CHECK_ENABLED=true