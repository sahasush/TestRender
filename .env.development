# Development Environment Configuration
# This file is used for local development

# API Configuration
VITE_API_URL=http://localhost:4000/
VITE_API_TIMEOUT=10000
VITE_API_KEY=dev-api-key-12345
VITE_API_VERSION=v1

# Application Settings
VITE_APP_NAME=Eirvana
VITE_APP_VERSION=1.0.0
VITE_APP_ENVIRONMENT=development

# Feature Flags
VITE_ENABLE_DEBUG=true
VITE_ENABLE_MOCK_AUTH=true
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_ERROR_REPORTING=false

# Authentication Settings
VITE_AUTH_TOKEN_EXPIRY=86400000
VITE_AUTH_REFRESH_THRESHOLD=300000

# UI Configuration
VITE_DEFAULT_THEME=light
VITE_ENABLE_THEME_TOGGLE=true
VITE_SHOW_VERSION_INFO=true

# Development Tools
VITE_ENABLE_REACT_DEVTOOLS=true
VITE_ENABLE_REDUX_DEVTOOLS=true
VITE_LOG_LEVEL=debug

# External Services (Development)
VITE_SENTRY_DSN=
VITE_GOOGLE_ANALYTICS_ID=
VITE_MIXPANEL_TOKEN=

# Health Check
VITE_HEALTH_CHECK_INTERVAL=30000
VITE_HEALTH_CHECK_ENABLED=true